#include <iostream>

using namespace std;
int main() {
    
int number = 9;
int Guessno;


while (guessno != number) {
        cout << "Enter your guess: ";
        cin >> userno;
        
        if (userno > number) {
            cout << "Too high! Try again." << endl;
        } else if (Guessno < number) {
            cout << "Too low! Try again." << endl;
        } else {
            cout << "Congratulations! You guessed the correct number." << endl;
        }
    }
    return 0;
}
